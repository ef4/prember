{
  "name": "prember",
  "version": "0.3.0-alpha.1",
  "description": "Prerender Ember apps using Fastboot at build time.",
  "keywords": [
    "ember-addon",
    "fastboot",
    "prerender"
  ],
  "license": "MIT",
  "author": "Edward Faulkner <edward@eaf4.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/ef4/prember",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "qunit node-tests"
  },
  "dependencies": {
    "broccoli-debug": "^0.6.3",
    "broccoli-merge-trees": "^2.0.0",
    "broccoli-plugin": "^1.3.0",
    "chalk": "^2.3.0",
    "denodeify": "^1.2.1",
    "ember-cli-babel": "^6.6.0",
    "express": "^4.16.2",
    "fastboot": "^1.1.0",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "broccoli-asset-rev": "https://github.com/ef4/broccoli-asset-rev#838f1b7186fde566d3e8010196c1f974423a4946",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~2.16.2",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-document-title": "^0.3.3",
    "ember-cli-eslint": "^4.0.0",
    "ember-cli-fastboot": "https://github.com/ef4/ember-cli-fastboot#48b94151e373bc33bcc3ad737a1e3e2a7e4ed8da",
    "ember-cli-head": "^0.4.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.0.0",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.0.0",
    "ember-source": "~2.16.0",
    "jsdom": "^11.5.1",
    "loader.js": "^4.2.3",
    "qunitjs": "^2.4.1"
  },
  "engines": {
    "node": "6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-cli-fastboot"
    ],
    "before": [
      "broccoli-asset-rev"
    ]
  }
}
