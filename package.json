{
  "name": "prember",
  "version": "1.0.3",
  "description": "Prerender Ember apps using Fastboot at build time.",
  "keywords": [
    "ember-addon",
    "fastboot",
    "prerender"
  ],
  "license": "MIT",
  "author": "Edward Faulkner <edward@eaf4.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/ef4/prember",
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember serve",
    "test": "eslint . && qunit node-tests"
  },
  "dependencies": {
    "broccoli-debug": "^0.6.3",
    "broccoli-merge-trees": "^3.0.2",
    "broccoli-plugin": "^2.1.0",
    "chalk": "^2.3.0",
    "ember-cli-babel": "^7.8.0",
    "express": "^4.16.2",
    "fastboot": "^2.0.2",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^3.0.0",
    "ember-ajax": "^5.0.0",
    "ember-cli": "^3.11.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-document-title-northm": "^1.0.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-fastboot": "^2.1.3",
    "ember-cli-head": "^0.4.0",
    "ember-cli-htmlbars": "^3.1.0",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-fetch": "^6.7.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^5.1.3",
    "ember-source": "^3.11.1",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.2.1",
    "eslint": "^6.0.1",
    "eslint-plugin-ember": "^6.7.1",
    "eslint-plugin-node": "^9.1.0",
    "jsdom": "^15.1.1",
    "loader.js": "^4.2.3",
    "qunitjs": "^2.4.1"
  },
  "engines": {
    "node": ">= 8.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-cli-fastboot",
      "ember-engines"
    ],
    "before": [
      "broccoli-asset-rev"
    ]
  }
}
